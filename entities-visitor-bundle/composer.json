{
    "name": "marbobley/entities-visitor-bundle",
    "description": "Manage visitor connexion information in Doctrine entities",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Marbobley\\EntitiesVisitorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marbobley\\EntitiesVisitorBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Marbobley",
            "email": "fournier.wilf@gmail.com"
        }
    ],
    "require": {
        "doctrine/orm": "^3.6",
        "symfony/http-foundation": "^6.4 || ^7.0",
        "symfony/http-kernel": "^6.4 || ^7.0",
        "psr/log": "^1.1 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5"
    }
}
